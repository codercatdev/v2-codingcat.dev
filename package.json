		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vitest": "^0.25.3",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
